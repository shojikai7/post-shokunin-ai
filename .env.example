# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Gemini API
GEMINI_API_KEY=your_gemini_api_key

# X (Twitter) OAuth
X_CLIENT_ID=your_x_client_id
X_CLIENT_SECRET=your_x_client_secret

# Meta (Instagram) OAuth
META_CLIENT_ID=your_meta_client_id
META_CLIENT_SECRET=your_meta_client_secret

# Google Business Profile OAuth
GBP_CLIENT_ID=your_gbp_client_id
GBP_CLIENT_SECRET=your_gbp_client_secret

# TikTok OAuth
TIKTOK_CLIENT_KEY=your_tiktok_client_key
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret

# LINE Login (OIDC)
LINE_CHANNEL_ID=your_line_channel_id
LINE_CHANNEL_SECRET=your_line_channel_secret

# Encryption Key (for token storage)
ENCRYPTION_KEY=your_32_byte_encryption_key

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Cloud Run Worker URL
WORKER_URL=your_cloud_run_worker_url

# GCP Pub/Sub
GCP_PROJECT_ID=your_gcp_project_id
GCP_PUBSUB_TOPIC=generation-jobs
